\relax

% lthj.etx -- install a LTH-encoded roman/thai font with oldstyle digits
%
% We do not need to distinguish between roman and italic in LTH,
% hence we simply call lth.etx with oldstyle parameters.

\encoding

\setcommand\lc#1#2{#2}
\setcommand\uc#1#2{#1}
\setcommand\lctop#1#2{#2}
\setcommand\uctop#1#2{#1}
\setcommand\lclig#1#2{#2}
\setcommand\uclig#1#2{#1}
\setcommand\digit#1{#1oldstyle}

\inputetx{LTH}

\endencoding
